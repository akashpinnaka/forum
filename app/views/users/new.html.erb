<%= form_for(:user, :url => {:action => "create"}) do |f| %>
	<%= f.label(:registered_id) %><br />
	<%= f.text_field(:registered_id) %><br />
	<%= f.label(:first_name) %><br />
	<%= f.text_field(:first_name) %><br />
	<%= f.label(:last_name) %><br />
	<%= f.text_field(:last_name) %><br />
	<%= f.label(:email) %><br />
	<%= f.text_field(:email) %><br />
	<%= f.label(:role) %><br />
	<%= f.select(:role, options_for_select([ "student", "admin" ], "student")) %><br />
	<%= f.label(:branch) %><br />
	<%= f.text_field(:branch) %><br />
	<%= f.label(:password) %><br />
	<%= f.password_field(:password) %><br />
	<%= f.submit("Create User") %>
<% end %>